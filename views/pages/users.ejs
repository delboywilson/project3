<h2>All users:</h2>
<table class="users">
  <tr>
    <td>First name:</td>
    <td>Last name:</td>
    <td>Email:</td>
    <td>Password:</td>
  </tr>
  <% for(var i=0; i < users.length; i++) { %>
  <tr>
    <td><%= users[i].firstname %></td>
    <td><%= users[i].lastname %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].password %></td>
  </tr>
  <% } %>
</table>
