<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Users website</title>
  <link rel="stylesheet" href="/static/main.css"/>
</head>
<body>
  <div class="content-wrapper">
    <div class="top-bar">
      <h1>Schedule website single</h1>
    </div>
    <div class="main-content">
      <table class="users">
        <tr>
          <td>User ID:</td>
          <td>Day:</td>
          <td>Start at:</td>
          <td>End at:</td>
        </tr>
        <% for(var i=0; i < scheduleId.length; i++) { %>
        <tr>
          <td><%= scheduleId[i].user_id %></td>
          <td><%= scheduleId[i].day %></td>
          <td><%= scheduleId[i].start_at %></td>
          <td><%= scheduleId[i].end_at %></td>
        </tr>
        <% } %>
      </table>
    </div>
</body>
</html>