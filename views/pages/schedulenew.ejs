<h3>Please add new schedule:</h3>
<div class="form-container">
  <form method="POST" action="/schedules">
    <ul>
      <li>
        <label for="user_id">User Id:</label>
        <input type="number" id="user_id" name="user_id" autocomplete="off" required>
        <small></small>
      </li>
      <li>
        <label for="day">Day:</label>
        <input type="number" id="day" name="day" autocomplete="off" required>
        <small></small>
      </li>
      <li>
        <label for="start_at">Start at:</label>
        <input type="text" id="start_at" name="start_at" autocomplete="off" required>
        <small></small>
      </li>
      <li>
        <label for="end_at">End at:</label>
        <input type="text" id="end_at" name="end_at" autocomplete="off" required>
        <small></small>
      </li>
      <li>
        <button class="button" id="button-open" type="submit">Submit</button>
      </li>
    </ul>
  </form>
</div>
