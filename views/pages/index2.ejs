<h1>All user schedules:</h1>
<div class="link-schedules">
  <a href="/new">Click here to add a new schedule:</a>
</div>
<ul class="user-schedules">
  <% for(let i =0; i < mySchedule.length; i++) { %>
    <li class="individual-users"><b>User name: </b><%= mySchedule[i].user_name %> <b>Day: </b><%= mySchedule[i].day %> <b>Start time: </b><%= mySchedule[i].start_time %> <b>End time: </b><%= mySchedule[i].end_time %></li>
  <%} %>
</ul>
